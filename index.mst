    </header>

    <section id="content" class="container">
      <div class="content-inner">
        {{#event-city}}{{#date}}
        <div class="content-details">
          <h2>
            {{{ event-city }}}
            <br />
            {{{ date }}}
          </h2>
        </div>
        {{/date}}{{/event-city}}
        <div class="content-headings">
          <h1>{{{ headline }}}</h1>
          <h3>{{{ subheading }}}</h3>
        </div>
      </div>
    </section>

    <footer id="rh-logo" class="container">
      <div class="rh-logo-img">
        <img src="assets/images/rh-logo.svg" />
      </div>
    </footer>
